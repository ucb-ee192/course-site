\usetheme{AnnArbor}
\usecolortheme{wolverine}
\usefonttheme[onlymath]{serif}
\setbeamertemplate{headline}[infolines theme]
\setbeamertemplate{navigation symbols}{}
\setbeamertemplate{itemize items}[default]
\setbeamertemplate{enumerate items}[default]
%\useinnertheme{rectangles}
\setbeamertemplate{section in toc}[square]
\setbeamertemplate{subsection in toc}[square]
\definecolor{light_red}{RGB}{255, 0, 0}
% UC Berkeley color palette
\definecolor{pantone_540_blue}{HTML}{00335B}
\definecolor{pantone_139_gold}{HTML}{AF7505}
\definecolor{pantone_874_metallic}{HTML}{8C6B47}
\definecolor{pantone_294_blue}{HTML}{003F87}
\definecolor{banner_bg_blue}{HTML}{1C2045}
\definecolor{nav_text_gold}{HTML}{E7C254}
\definecolor{nav_bg_blue}{HTML}{252C58}
\definecolor{nav_hover_bg_blue}{HTML}{5E6489}
\definecolor{nav_hover_text_gold}{HTML}{F8DA73}
\definecolor{flyover_bg_gray}{HTML}{808B9D}
\definecolor{highlight_bg_gold}{HTML}{C19E36}
\definecolor{highlight_text_brown}{HTML}{5D461B}
\definecolor{highlight_text_gold}{HTML}{E8C62E}
\definecolor{topic_bg_orange}{HTML}{D57D2D}
\definecolor{topic_bg_purple}{HTML}{9C5D72}
\definecolor{topic_bg_green}{HTML}{97AF81}
% Title
\setbeamercolor{title}{fg = white, bg = pantone_540_blue}
\setbeamercolor{subtitle}{fg = pantone_139_gold, bg = pantone_139_gold}
% Frame
\setbeamercolor{frametitle}{fg = white, bg = pantone_540_blue}
\setbeamercolor{framesubtitle}{fg = pantone_139_gold, bg = pantone_139_gold}
% Block
\setbeamercolor{block title}{fg = white, bg = pantone_540_blue}
\setbeamercolor{block title example}{fg = white, bg = pantone_540_blue}
\setbeamercolor{example text}{fg = pantone_540_blue}
% Normal text & background
\setbeamercolor{normal text}{bg = white}
\setbeamercolor{background canvas}{bg = white}
% Header & footer
\setbeamercolor{section in head/foot}{fg = white, bg = pantone_139_gold}
\setbeamercolor{subsection in head/foot}{fg = white, bg = pantone_540_blue}
\setbeamercolor{author in head/foot}{fg = white, bg = pantone_139_gold}
\setbeamercolor{title in head/foot}{fg = white, bg = pantone_540_blue}
\setbeamercolor{date in head/foot}{fg = white, bg = pantone_139_gold}
% alert
\setbeamercolor{alerted text}{fg = pantone_139_gold}
\setbeamerfont{alerted text}{shape=\itshape}
% itemize
\setbeamercolor{itemize item}{fg = pantone_540_blue}
\setbeamercolor{itemize subitem}{fg = pantone_540_blue}
\setbeamercolor{itemize subsubitem}{fg = pantone_540_blue}
% enumerate
\setbeamercolor{enumerate item}{fg = pantone_540_blue}
\setbeamercolor{enumerate subitem}{fg = pantone_540_blue}
\setbeamercolor{enumerate subsubitem}{fg = pantone_540_blue}
% figure
\setbeamercolor{caption}{fg = black}
\setbeamercolor{caption name}{fg = pantone_139_gold}
% TOC
\setbeamercolor{section in toc}{fg = pantone_139_gold, bg = white}
\setbeamercolor{subsection in toc}{fg = pantone_540_blue, bg = white}
\setbeamercolor{section number projected}{bg=pantone_540_blue, fg=pantone_540_blue}
\setbeamercolor{subsection number projected}{bg=pantone_540_blue, fg=pantone_540_blue}
% Bibliography
\setbeamercolor{bibliography entry author}{fg = pantone_540_blue, bg = white}
\setbeamercolor{bibliography entry title}{fg = pantone_540_blue, bg = white}
\setbeamercolor{bibliography entry location}{fg = pantone_540_blue, bg = white}
\setbeamercolor{bibliography entry note}{fg = pantone_540_blue, bg = white}
\setbeamercolor{bibliography item}{fg = pantone_540_blue, bg = white}
